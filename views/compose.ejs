<%- include("partials/header") -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="postTitle">Title</label><br>
            <input class="form-control" type="text" name="postTitle"><br>
            <label for="postBody">Post</label><br>
            <textarea class="form-control" rows="5" cols="30" name="postBody"></textarea><br>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
<%- include("partials/footer") -%>
